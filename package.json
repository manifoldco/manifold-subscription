{
  "name": "manifold-subscription",
  "version": "0.0.1",
  "description": "Manifold Subscription Web Components",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/manifold-subscription/manifold-subscription.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "generate:gql": "graphql-codegen --config codegen.yml",
    "happo": "npm run build && happo run",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:js": "eslint --ext .js,.ts,.tsx src"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.13.1",
    "@graphql-codegen/typescript": "^1.13.1",
    "@graphql-codegen/typescript-operations": "^1.13.1",
    "@manifoldco/eslint-plugin-stencil": "^0.4.1",
    "@manifoldco/manifold-init-types": "^0.3.0",
    "@manifoldco/mercury": "0.1.0-beta.3",
    "@rollup/plugin-replace": "^2.3.1",
    "@stencil/core": "^1.10.1",
    "@stencil/postcss": "^1.0.1",
    "@stencil/sass": "^1.1.1",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "graphql": "^14.6.0",
    "happo-plugin-typescript": "^1.0.0",
    "happo.io": "^5.1.5",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "rollup-pluginutils": "^2.8.2",
    "stylelint": "^13.2.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-config-standard-scss": "^1.1.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.16.0"
  },
  "peerDependencies": {
    "@manifoldco/manifold-init": "*"
  },
  "license": "MIT",
  "dependencies": {
    "@stripe/stripe-js": "^1.2.0"
  }
}
